        <img class="card-img-top card-img-artist" src=<%= artist.image_url %>>
        <div class="card-body">
          <h4 class="card-title"><%= artist.name %></h4>
          <a href="#" class="btn btn-primary">Ver más</a>
        </div>
      </div>
    </div>
    <% end %>

  </div>
</div>